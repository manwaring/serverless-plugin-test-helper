{
  "name": "serverless-plugin-test-helper",
  "description": "A utility library to help with testing deployed serverless applications",
  "version": "1.1.2",
  "scripts": {
    "publish-please-dry-run": "publish-please --dry-run",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard && npm run build",
    "build": "rimraf dist && tsc -p ./tsconfig.build.json",
    "postbuild": "ncp src/index.d.ts dist/index.d.ts",
    "test": "nyc node_modules/cucumber/bin/cucumber-js -p default",
    "pretest": "rimraf .test-output .serverless dist",
    "posttest": "rimraf .test-output .serverless .test.* && nyc report --reporter=json --reporter=lcov",
    "codecov": "codecov -f coverage/*.json",
    "testexamples-default": "cd examples/default && npm i && npm run deploy && npm test",
    "testexamples-custom": "cd examples/custom && npm i && npm run deploy && npm test"
  },
  "dependencies": {
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^12.6.9",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.2",
    "cucumber-tsflow": "^3.2.0",
    "lodash.clonedeep": "^4.5.0",
    "ncp": "^2.0.0",
    "nyc": "^14.1.1",
    "publish-please": "^5.5.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manwaring/serverless-testing.git"
  },
  "bugs": {
    "url": "https://github.com/manwaring/serverless-testing"
  },
  "homepage": "https://github.com/manwaring/serverless-testing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "From Phillip with â™¥",
  "license": "MIT",
  "keywords": [
    "serverless",
    "plugin",
    "testing",
    "library",
    "helper"
  ]
}
